{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "codepipeline:*",
            "Resource": "*"
        },
		{
            "Effect": "Allow",
            "Action": [
                "lambda:InvokeFunction",
                "ec2:Describe*"
            ],
            "Resource": "*"
        },
		{
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": "*"
        }
    ]
}